  <message code="6" type="warn" param="Timeout"/>
  <message code="7" type="error" param="Could not open UDP socket"/>
  <message code="8" type="info" param=""/>
  <message code="9" type="info" param=""/>
  <message code="10" type="info" param=""/>
</debugHandler>

